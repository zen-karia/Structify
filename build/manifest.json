{
  "manifest_version": 3,
  "name": "Structify",
  "version": "1.0.0",
  "description": "AI-powered Chrome extension to organize and jump to tabs in real time.",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "https://api.openai.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/Structify.png",
      "32": "assets/Structify.png",
      "48": "assets/Structify.png",
      "128": "assets/Structify.png"
    }
  },
  "icons": {
    "16": "assets/Structify.png",
    "32": "assets/Structify.png",
    "48": "assets/Structify.png",
    "128": "assets/Structify.png"
  }
}
